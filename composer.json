{
    "name": "hyperf/nacos",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "php",
        "hyperf",
        "nacos"
    ],
    "description": "Nacos SDK",
    "autoload": {
        "psr-4": {
            "Hyperf\\Nacos\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "HyperfTest\\Nacos\\": "tests"
        }
    },
    "require": {
        "php": ">=8.1",
        "guzzlehttp/guzzle": "^6.5|^7.0",
        "hyperf/codec": "~3.1.0",
        "hyperf/contract": "~3.1.0",
        "hyperf/support": "~3.1.0",
        "hyperf/utils": "~3.1.0"
    },
    "suggests": {
        "google/protobuf": "Required to use nacos v2 grpc apis.",
        "hyperf/grpc": "Required to use nacos v2 grpc apis.",
        "hyperf/http2-client": "Required to use nacos v2 grpc apis."
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0-dev"
        },
        "hyperf": {
            "config": "Hyperf\\Nacos\\ConfigProvider"
        }
    }
}
